<Page x:Class="WPFTETRIS.AI"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPFTETRIS"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AI" Background="Black">

    <Grid x:Name="single">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <Grid x:Name="Tetris" Grid.Column="0">
            <Frame x:Name="T_Board" NavigationUIVisibility="Hidden"/>
        </Grid>
        <Grid Name="Score" Grid.Column="1">
            <Frame x:Name="S_Board" NavigationUIVisibility="Hidden"/>
        </Grid>
    </Grid>
</Page>
